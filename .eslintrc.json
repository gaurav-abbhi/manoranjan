{
    "extends": ["airbnb","prettier"],
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "prettier"
    ],
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "react/forbid-prop-types": 0,
        "react/prop-types": 0,
        "jsx-a11y/label-has-for": 0,
        "no-console": 1,
        "prettier/prettier": ["error"]
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    }
}
